page{
	height:100%;
}

@font-face {
  font-family: 'iconfont';
  src: url('https://at.alicdn.com/t/font_1446288_7wmh4c92ql5.ttf') format('truetype');
}

.index-wrap {
  height: 100%;
  background: #f8f8f8;
}
.index-wrap .scroll-musicList {
  position: absolute;
  top: 140rpx;
  left: 0;
  bottom: 0;
  width: 100%;
}
.index-wrap .hideNav {
  height: 100rpx;
  visibility: hidden;
  opacity: 0;
}
.index-wrap .vs.fix {
  position: fixed;
  width: 100%;
  left: 0;
  top: 140rpx;
  z-index: 10;
}

